GetGlobal.Owner = "DekuDimz"
GetGlobal.Helper = "Bluwu"
GetGlobal.User = Instance.new("VirtualInputManager")
GetGlobal.AFK = false
GetGlobal.Character = Player.Character or Player.CharacterAdded:Wait()
