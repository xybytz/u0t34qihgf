    Strings = Options.N.Value
end)
LeftGroupBox:AddToggle("NIGdGxER", {Text = 'Auto Incubate', Default = false,Tooltip = ''})
Toggles.NIGdGxER:OnChanged(function(State)
    AutoFish = Toggles.NIGdGxER.Value
    spawn(function()
        while task.wait() do
            pcall(function()
                if AutoFish then
                    Fish:StartCasting()
                    if Fish.Snagged then
                        Fish.ReelSignal:Fire()
                        if Player.PlayerGui.FishingGui.ContainerFrame.ReelingFrame.BarLabel.InnerLabel.PointerLabel.Position.X.Scale <= Player.PlayerGui.FishingGui.ContainerFrame.ReelingFrame.BarLabel.InnerLabel.SafeBarLabel.Position.X.Scale + .02 then
                            Fish:Click()
                        end
                    end
                end
            end)
        end
    end)
end)


