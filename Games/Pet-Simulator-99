    "Muscular",
    "Sensitive",
    "Balanced"
}
local Races = {
    "Human",
    "Kumate Tribe",
    "Skypiean",
    "Torino Tribe",
    "Vermin",
    "A Bizarre Human",
    "Human (Hybrid)",
    "Mink",
