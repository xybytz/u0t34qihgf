local A_2 = 
{
	[1] = game:GetService("ReplicatedStorage").Storage.Buildings.Food["Plant_Apple"], 
	[2] = CFrame.new(-49, 7, 14951), 
	[3] = false
}
local Event = game:GetService("Players")[UsernameSpot].Function
Event:InvokeServer(A_1, A_2)

end    
end)   

