local function GetDragons()
  local Target
    for i, v in ipairs(game.Players.LocalPlayer.Character.Dragons:GetChildren()) do
        if v.ClassName == "Model" and v:FindFirstChild("HumanoidRootPart")  then
            Target = v
        end
    end
    return Target
end 
