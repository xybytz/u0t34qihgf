CategoryVariableHere:Toggle("Make Apple plant automaticly",function(State) 
nigger11=State    
while nigger11 do wait()
local args = {
    [1] = "Craft",
    [2] = "Plant_Apple"
}

game:GetService("Players").LocalPlayer.Function:InvokeServer(unpack(args))
