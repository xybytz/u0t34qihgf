CategoryVariableHere:Button("Fast levels",function()

game:GetService('RunService').RenderStepped:connect(function()
 
local args = {
    [1] = "Craft",
    [2] = "Plant_Apple"
}
game:GetService("Players").LocalPlayer.Function:InvokeServer(unpack(args))
local args = {
    [1] = "SalvageBuilding",
    [2] = {
        [1] = "Plant_Apple",
        [2] = 1
    }
}
game:GetService("Players").LocalPlayer.Remote:FireServer(unpack(args)) end)
 end)






